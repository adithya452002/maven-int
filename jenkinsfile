pipeline {
    agent any
    tools{
        maven "maven"
    }
    stages {
        stage('source code') {
            steps {
                git 'https://github.com/adithya452002/maven-int.git'
            }
        }
        stage("compile"){
            steps {
                sh "mvn compile"
            }
        }
        stage("test") {
            steps {
                sh "mvn test"
            }
        }
        stage("Build") {
            steps {
                sh "mvn package"
            }
        }
    }
}
